; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Bubble pop 3.5"
run/main_scene="res://Main menu.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"

[autoload]

LevelCrud="*res://create level script/Level CRUD.gd"
SceneManager="*res://SceneManager.tscn"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/size/window_width_override=360
window/size/window_height_override=640
window/stretch/mode="viewport"
window/handheld/orientation=1

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
environment/defaults/default_environment="res://default_env.tres"
